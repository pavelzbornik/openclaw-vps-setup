{
  "agent": {
    "model": "{{ openclaw_config.agent.model }}"
  },
  "gateway": {
    "bind": "{% if tailscale_enabled | default(false) and openclaw_app_tailscale_ip_result is defined and openclaw_app_tailscale_ip_result.stdout | default('') %}{{ openclaw_app_tailscale_ip_result.stdout | trim }}{% else %}{{ openclaw_config.gateway.bind }}{% endif %}",
    "port": {{ openclaw_config.gateway.port }},
    "auth": {
      "mode": "{{ openclaw_config.gateway.auth.mode }}",
      "token": "{{ openclaw_config.gateway.auth.token }}"
    }
  },
  "channels": {
    "telegram": {
      "enabled": {{ openclaw_config.channels.telegram.enabled | lower }},
      "dmPolicy": "{{ openclaw_config.channels.telegram.dmPolicy }}"
    },
    "discord": {
      "enabled": {{ openclaw_config.channels.discord.enabled | lower }},
      "dmPolicy": "{{ openclaw_config.channels.discord.dmPolicy }}"
    }
  },
  "workspace": {
    "path": "{{ openclaw_config_dir }}/workspace"
  },
  "logging": {
    "level": "info",
    "path": "{{ openclaw_config_dir }}/logs"
  }
}
