# Example Terraform variables file
# Copy this to terraform.tfvars and fill in your actual values
# NEVER commit terraform.tfvars to version control!

# 1Password vault name where Discord secrets are stored
onepassword_vault = "OpenClaw"

# 1Password item name containing Discord credentials
# This item should have fields:
# - credential: your Discord bot token
# - server_id: your Discord server (guild) ID (single ID, not a list)
# - allowlist: comma-separated user IDs (used by OpenClaw at runtime)
# - guilds: comma-separated guild IDs (used by OpenClaw at runtime)
onepassword_discord_item = "discord"
